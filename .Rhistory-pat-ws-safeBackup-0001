Prob <- function (axes = F, ...)
{
br1 <- brk[-(k + 1)]
br2 <- brk[-1]
if (!exists("nomex"))
nomex <- ""
plot(range(brk), range(c(0, dat2$hj), na.rm = T), type = "n",
axes = F, xlab = nomex, ylab = "Prob")
